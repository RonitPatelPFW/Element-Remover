{
    "name": "Element Remover",
    "description": "Removes elements of choosing",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [ 
      "activeTab", 
      "scripting",
      "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }